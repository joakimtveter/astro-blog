---
import Angular from '../assets/technologies/angular.svg?raw';
import Svelte from '@/assets/technologies/svelte.svg?raw';
import CSharp from '@/assets/technologies/c-sharp.svg?raw';
import Umbraco from '@/assets/technologies/umbraco.svg?raw';
import DotNet from '@/assets/technologies/dotnet.svg?raw';
interface Props {
    name: string;
}

const { name} = Astro.props;
---

{name === 'angular' ? <Fragment set:html={Angular} style="height: 100%; width: 100%"/> : null}
{name === 'svelte' ? <Fragment set:html={Svelte} style="height: 100%; width: 100%"/> : null}
{name === 'umbraco' ? <Fragment set:html={Umbraco} style="height: 100%; width: 100%"/> : null}
{name === 'c-sharp' ? <Fragment set:html={CSharp} style="height: 100%; width: 100%"/> : null}
{name === 'dotnet' ? <Fragment set:html={DotNet} style="height: 100%; width: 100%"/> : null}

<style>
    svg {
        border 1px solid red
    }
</style>
